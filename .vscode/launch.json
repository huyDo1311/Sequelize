{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "BE Cyber Media",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "runtimeExecutable": "nodemon",
            "runtimeArgs": [
                "--experimental-modules"
            ],
            "console": "integratedTerminal"
        }
    ]
}
